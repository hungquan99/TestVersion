-- ROBLOX_SCRIPT_FROM_URL_ONLY

local source = debug.getinfo(1, "S").source

-- Detect if executed directly (from clipboard/paste) instead of from a URL
if not tostring(source):find("http") then
    return warn("This script must be run using loadstring(game:HttpGet('...'))(). Direct execution is blocked.")
end

