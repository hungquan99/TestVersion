-- ROBLOX_SCRIPT_FROM_URL_ONLY

local info = debug.getinfo(1, "S")
local source = tostring(info and info.source or "")

-- If the script was loaded via loadstring(game:HttpGet(...))(),
-- the source will usually be like: [string "HttpGet"] or similar,
-- NOT like: [string "print('hello')"] or [string "line 1"]
if not source:lower():find("httpget") and not source:lower():find("loadstring") then
    return warn("This script must be run using loadstring(game:HttpGet('...'))(). Direct execution is blocked.")
end

print("âœ… Script loaded successfully from URL!")
